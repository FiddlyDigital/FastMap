{
  "name": "@speakingsoftware/fastmap",
  "version": "1.0.0",
  "description": "A 2d Array optimized for speed and memory usage.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "homepage": "https://www.npmjs.com/package/@speakingsoftware/fastmap",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FiddlyDigital/FastMap.git"
  },
  "bugs": {
    "url": "https://github.com/FiddlyDigital/FastMap/issues"
  },
  "author": "SpeakingSoftware / Philip Gannon (https://www.speakingsoftwareshow.com/)",
  "license": "ISC",
  "keywords": [
    "map",
    "array",
    "cimage",
    "data"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "test": "jest tests --verbose --config=./tests/jest.config.cjs"
  },
  "tsup": {
    "target": "es5"
  },
  "devDependencies": {
    "@swc/core": "^1.10.12",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3"
  },
  "directories": {
    "test": "tests"
  },
  "dependencies": {
  }
}
