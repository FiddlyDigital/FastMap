{
    "name": "@speakingsoftware/fastmap",
    "version": "1.0.0",
    "description": "A 2d Map optimized for speed and memory usage.",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.js",
            "import": "./dist/index.mjs"
        }
    },
    "files": [
        "dist",
        "README.md",
        "package.json"
    ],
    "homepage": "https://www.npmjs.com/package/@speakingsoftware/fastmap",
    "repository": {
        "type": "git",
        "url": "https://github.com/FiddlyDigital/FastMap.git"
    },
    "bugs": {
        "url": "https://github.com/FiddlyDigital/FastMap/issues"
    },
    "author": {
        "name": "SpeakingSoftware / Philip Gannon",
        "url": "https://www.speakingsoftwareshow.com/"
    },
    "license": "ISC",
    "keywords": [
        "map",
        "array",
        "cimage",
        "data"
    ],
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts --clean"
    },
    "tsup": {
        "target": "es5"
    },
    "devDependencies": {
        "@swc/core": "^1.10.12",
        "tsup": "^8.3.6",
        "typescript": "^5.7.3"
    }
}
